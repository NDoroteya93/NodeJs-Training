#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/C:/Users/Dori/AppData/Local/Yarn/cache/v1/npm-mocha-3.4.2-d0ef4d332126dbf18d0d640c9b382dd48be97594/bin/mocha" "$@"
  ret=$?
else 
  node  "$basedir/C:/Users/Dori/AppData/Local/Yarn/cache/v1/npm-mocha-3.4.2-d0ef4d332126dbf18d0d640c9b382dd48be97594/bin/mocha" "$@"
  ret=$?
fi
exit $ret
